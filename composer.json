{
    "require": {
        "php": "^7.4",
        "symfony/symfony": "5.4.*",
        "doctrine/orm": "^2.7",
        "symfony/framework-bundle": "^5.4",
        "twig/twig": "^3.0",
        "symfony/mailer": "^5.4",
        "sensio/framework-extra-bundle": "^5.6",
        "majora/framework-extra-bundle": "^1.0"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "scripts": {
        "post-install-cmd": [
            "php bin/console_demo cache:clear --env=dev",
            "php bin/console_demo cache:warmup --env=dev"
        ],
        "post-update-cmd": [
            "php bin/console_demo cache:clear --env=dev",
            "php bin/console_demo cache:warmup --env=dev"
        ]
    },
    "extra": {
        "incenteev-parameters": {
            "file": "apps/demo/config/parameters.yml"
        }
    }
}
