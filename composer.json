{
    "require": {
        "php": "^7.4",
        "symfony/symfony": "5.4.*",
        "doctrine/orm": "^2.7",
        "symfony/framework-bundle": "^5.4",
        "twig/twig": "^3.0",
        "symfony/mailer": "^5.4",
        "sensio/framework-extra-bundle": "^5.6",
        "majora/framework-extra-bundle": "^1.0",
        "symfony/monolog-bundle": "^3.7",
        "symfony/swiftmailer-bundle": "^3.5",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "stof/doctrine-extensions-bundle": "^1.6"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "scripts": {
        "post-install-cmd": [
            "php bin/console_demo cache:clear --env=dev",
            "php bin/console_demo cache:warmup --env=dev"
        ],
        "post-update-cmd": [
            "php bin/console_demo cache:clear --env=dev",
            "php bin/console_demo cache:warmup --env=dev"
        ]
    },
    "extra": {
        "incenteev-parameters": {
            "file": "apps/demo/config/parameters.yml"
        }
    },
    "autoload": {
        "psr-4": {
            "Synapse\\Cmf\\Bundle\\": "src/Synapse/Cmf/Bundle/",
            "Synapse\\Admin\\Bundle\\": "src/Synapse/Admin/Bundle/",
            "Synapse\\Page\\Bundle\\": "src/Synapse/Page/Bundle/",
            "Synapse\\Demo\\Bundle\\": "src/Synapse/Demo/Bundle/"
        }
    }
}
